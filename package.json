{
  "author": "planttheidea",
  "browser": "dist/moize.js",
  "browserslist": [
    "defaults",
    "Explorer >= 9",
    "Safari >= 6",
    "Opera >= 15",
    "iOS >= 8",
    "Android >= 4"
  ],
  "bugs": {
    "url": "https://github.com/planttheidea/moize/issues"
  },
  "dependencies": {
    "fast-equals": "^1.6.2",
    "fast-stringify": "^1.1.0",
    "jest-codemods": "^0.19.1",
    "micro-memoize": "^3.0.1"
  },
  "description": "Blazing fast memoization based on all parameters passed",
  "devDependencies": {
    "@babel/preset-typescript": "^7.3.3",
    "@types/jest": "^24.0.5",
    "@types/lodash": "^4.14.121",
    "@types/node": "^11.9.4",
    "@types/ramda": "^0.25.50",
    "@types/react": "^16.8.3",
    "@types/react-dom": "^16.8.1",
    "@types/react-virtualized": "^9.18.12",
    "@typescript-eslint/eslint-plugin": "^1.2.0",
    "@typescript-eslint/parser": "^1.2.0",
    "benchee": "^1.1.0",
    "cli-table2": "^0.2.0",
    "eslint": "^5.14.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "fast-memoize": "^2.5.1",
    "html-webpack-plugin": "^3.2.0",
    "in-publish": "^2.0.0",
    "ink-docstrap": "^1.3.2",
    "jest": "^24.0.0",
    "jsdoc": "^3.5.5",
    "jsdoc-babel": "^0.5.0",
    "lodash": "^4.17.11",
    "lru-memoize": "^1.0.2",
    "mem": "^4.1.0",
    "memoizee": "^0.4.14",
    "memoizerific": "^1.11.3",
    "nyc": "^13.3.0",
    "prop-types": "^15.7.2",
    "ramda": "^0.26.1",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-virtualized": "^9.21.0",
    "remeasure": "^3.1.1",
    "rollup": "^1.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-typescript": "^1.0.0",
    "rollup-plugin-typescript2": "^0.19.2",
    "ts-jest": "^23.10.4",
    "ts-loader": "^5.3.3",
    "typescript": "^3.2.4",
    "underscore": "^1.9.1",
    "webpack": "^4.29.4",
    "webpack-cli": "^3.2.0",
    "webpack-dev-server": "^3.1.5"
  },
  "homepage": "https://github.com/planttheidea/moize#readme",
  "keywords": [
    "cache",
    "expire",
    "lru",
    "memoize",
    "memoization",
    "optimize",
    "performance",
    "promise",
    "ttl"
  ],
  "license": "MIT",
  "main": "dist/moize.cjs.js",
  "module": "dist/moize.esm.js",
  "name": "moize",
  "nyc": {
    "exclude": [
      "**/plugins/*.js",
      "**/test/*.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/planttheidea/moize.git"
  },
  "scripts": {
    "benchmark": "npm run dist -- --no-comments && node benchmark/index.js",
    "build": "NODE_ENV=production rollup -c",
    "clean": "npm run clean:dist && npm run clean:docs",
    "clean:dist": "rimraf dist",
    "clean:docs": "rimraf docs",
    "clean:mjs": "rimraf mjs",
    "copy:mjs": "npm run clean:mjs && node ./es-to-mjs.js",
    "dev": "NODE_ENV=development webpack-dev-server --colors --progress --config=webpack/webpack.config.js",
    "dist": "npm run clean:dist && npm run build",
    "docs": "npm run clean:docs && jsdoc -c ./jsdoc.config.json",
    "lint": "NODE_ENV=test eslint src/*.ts",
    "lint:fix": "NODE_ENV=test eslint src --fix",
    "postpublish": "npm run docs && git add . && git commit -m \"Update docs\" && git push",
    "prepublish": "if in-publish; then npm run prepublish:compile; fi",
    "prepublish:compile": "npm run lint && npm run test:coverage && npm run dist && npm run copy:mjs",
    "release": "release-it",
    "release:beta": "release-it --config=.release-it.beta.json",
    "release:dry": "release-it --dry-run",
    "start": "npm run dev",
    "test": "NODE_PATH=. jest",
    "test:coverage": "npm run test -- --coverage",
    "test:watch": "npm run test -- --watch"
  },
  "sideEffects": false,
  "types": "./index.d.ts",
  "version": "5.4.1"
}
